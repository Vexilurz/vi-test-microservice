# Временная зона
WORKSPACE_TIMEZONE=Europe/Moscow

# XDEBUG
DOCKER_PHP_ENABLE_XDEBUG='on'
# Port and host IP of your PC in local network
XDEBUG_TARGET_HOST="192.168.1.20"
XDEBUG_TARGET_PORT=9001

# Настройки Nginx
# Порт, который следует использовать
# для соединения с локального компьютера
NGINX_PORT=80

# Настройки MySQL
MYSQL_ROOT_PASSWORD="root"
MYSQL_DATABASE="vitm"
MYSQL_USER="mysql"
MYSQL_PASSWORD="mysql"
# Порт, который следует использовать
# для соединения с локального компьютера
MYSQL_PORT=4307

# Настройки PHP
# Внешний порт, доступен с локального компьютера
PHP_PORT=9004
